<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="iut.lp.dba.contactwithloadermanager">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name=".activity.ContactsMainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AddNewContactActivity" >
        </activity>
        <provider
            android:name="iut.lp.dba.contactwithloadermanager.provider.PersonalContactProvider"
            android:authorities="lpirm.bdmob.contactmanager.provider">
        </provider>
    </application>
</manifest>